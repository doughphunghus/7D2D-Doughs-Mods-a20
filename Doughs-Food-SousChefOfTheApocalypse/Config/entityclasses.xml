<Doughs>

  <!-- Make the milk(s) low harvest, since we cant target dog gender,
  we want players to try to harvest frequently,
  and we don't want to be swimming in raw dog milk ;)
  -->
  <append xpath="/entity_classes/entity_class[@name='animalZombieDog']">
    <drop event="Harvest" name="doughsDrinkRawMilk" count="0" tool_category="Butcher"/> <!-- No get item -->
    <drop event="Harvest" name="doughsDrinkRawMilk" count="1" tag="butcherHarvest"/>
